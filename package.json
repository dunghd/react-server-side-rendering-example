{
  "name": "app-cards",
  "version": "1.1.0",
  "description": "Server-side Rendering React from Scratch!",
  "main": "index.js",
  "scripts": {
    "start-hypernova": "npx babel-node hypernova-server.js",
    "start-app": "npx babel-node app.js",
    "start": "concurrently \"npm run start-hypernova\" \"npm run start-app\"",
    "build": "webpack --config webpack.config.js",
    "format": "prettier --write \"src/**/*.{js,jsx}\""
  },
  "author": "Rohit kumar",
  "license": "MIT",
  "dependencies": {
    "@babel/node": "^7.26.0",
    "@babel/register": "^7.8.6",
    "axios": "^1.8.2",
    "express": "^4.17.1",
    "hypernova": "^2.5.0",
    "hypernova-react": "^2.1.0",
    "react": "^16.13.0",
    "react-dom": "^16.13.0",
    "react-redux": "^7.2.0",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.26.4",
    "@babel/core": "^7.26.9",
    "@babel/preset-env": "^7.26.9",
    "@babel/preset-react": "^7.26.3",
    "babel-loader": "^8.0.6",
    "concurrently": "^9.1.2",
    "prettier": "^3.5.3",
    "webpack": "^5.98.0",
    "webpack-cli": "^6.0.1"
  }
}
